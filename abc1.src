procedure GuardarTarasEnMatriz;
contadores : integer := 1 ;
begin
    BD_Cal.GetFirst; 
    MatrizPesos[1, contadores]  :=  BD_Cal.Peso;
    while(BD_Cal.GetNext <> SysNoSuchRecord or BD_Cal.Rodo < 1)
    loop 
        contadores := contadores + 1;
