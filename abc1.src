        -- Tensin ms peso meta
        if (Basculas[Basc].LecMax - Basculas[Basc].LecMin) > (2) then
            Basculas[Basc].Contador := Basculas[Basc].Contador + 1;
            MatrizPesos[Basc, Basculas[Basc].Contador] := Basculas[Basc].LecMax;
            GuardarPruebasEnBD(Basc,2);
